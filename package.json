    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.7",
    "react-scroll": "^1.4.5",
    "react-select": "^1.0.0-rc.3",
